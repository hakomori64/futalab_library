# APIが動作するサーバーのURL
HOST=http://localhost:3001
# APIが動作するポート
PORT=3001
# URLのプレフィックスです。apiが$HOST:$PORT/$SERVER_PREFIXで動作します
SERVER_PREFIX=api

# データベースの設定
DATABASE_HOST=mysql
DATABASE_PORT=3306
DATABASE_USERNAME=root
DATABASE_PASSWORD=password
DATABASE_NAME=library

# auth0の情報をセットしてください
AUTH0_DOMAIN=
AUTH0_AUDIENCE=
AUTH0_CLIENT_ID=
AUTH0_CLIENT_SECRET=